<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.aqualife.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/activity_home" />

    <fragment
        android:id="@+id/navigation_shopping_cart"
        android:name="com.example.aqualife.ui.cart.CartFragment"
        android:label="@string/title_cart"
        tools:layout="@layout/activity_all_shopping_cart" />

    <fragment
        android:id="@+id/navigation_order"
        android:name="com.example.aqualife.ui.order.OrderFragment"
        android:label="@string/title_order"
        tools:layout="@layout/activity_view_order" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.aqualife.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/activity_user_profile" />


</navigation>